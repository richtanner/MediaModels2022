<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/moviepopcorn"
        android:label="@string/app_name"
        android:roundIcon="@drawable/moviepopcorn"
        android:supportsRtl="true"
        android:theme="@style/Theme.MediaModels2022"
        android:name=".App"
        tools:targetApi="31">
        <activity
            android:name=".DavisMovieActivity"
            android:exported="false"
            android:parentActivityName=".ItemDetailHostActivity"
            >
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />

        </activity>
        <activity
            android:name=".ItemDetailHostActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:resizeableActivity="true"
            tools:targetApi="31">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
    </application>

</manifest>